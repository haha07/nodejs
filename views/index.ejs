
<% include layout/header.ejs %>

<section id="featured">
	<!-- start slider -->
	<div class="container">
		<div class="row">
			<div class="col-lg-12">
				<!-- Slider -->
		        <div id="main-slider" class="flexslider">
		            <ul class="slides">
		            	<% data.forEach(function (item, index) { %>
						<li>
							<img src="/slides/<%= item.file_name %>" alt="" />
							<div class="flex-caption">
								<h3><%= item.title %></h3> 
								<p><%= item.paragraph %></p> 
								<a href="#" class="btn btn-theme">Learn More</a>
							</div>
						</li>
						<% }); %>
		            </ul>
		        </div>
				<!-- end slider -->
			</div>
		</div>
	</div>	
</section>
